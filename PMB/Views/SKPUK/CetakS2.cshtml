    Layout = null;
}
@model CetakSKPUKView

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Surat Ketetapan Pembayaran Uang Kuliah @Model.DataMhs.nm_calon</title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback" />
    <!-- Font Awesome -->
    <link rel="stylesheet"
          href="/Assets/plugins/fontawesome-free/css/all.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <!-- Ionicons -->
    <link rel="stylesheet"
          href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" />
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet"
          href="/Assets/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" />
    <!-- Theme style -->
    <link rel="stylesheet" href="/Assets/dist/css/adminlte.min.css" />

    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        #namaMhs table, #namaMhs th, #namaMhs td {
            border-style: hidden;
        }

        #tblJenisBayar {
            margin-bottom: 40px;
        }

        #tblTotalBayar {
            margin-bottom: 10px;
        }

        #tblJenisBayar thead, #tblTotalBayar thead, #tblAngsuran thead {
            text-align: center;
        }

        #tblJenisBayar td, #tblTotalBayar td, #tblAngsuran td {
            padding: 5px;
            border: 1px solid black;
        }

        #tblJenisBayar tr, #tblTotalBayar tr, #tr td {
            padding: 5px;
            border: 1px solid black;
        }

        #isiJenisBayar {
            border-bottom: hidden;
        }

        #isiJmlJenisBayar {
            text-align: right;
            border-bottom: hidden;
        }

        #isiJenisPotongan {
            border-bottom: hidden;
        }

        #isiTotalBayar {
            text-align: right;
            border-bottom: hidden;
        }

        #ketSebelum {
            text-align: left;
        }

        #jmlSebelum {
            text-align: right;
        }

        #potSPU {
            text-align: left;
        }

        #jmlPotongan {
            text-align: right;
        }

        #ketSetelah {
            text-align: left;
        }

        #jmlSetelah {
            text-align: right;
        }

        #tblAngsuran {
            font-size:10px;
        }

        #noAngsuran {
            text-align: center;
        }

        #jmlAngsuran {
            text-align: right;
        }

        #jmlBayar {
            text-align: justify;
            float: left;
            width: 108%;
        }

        #ketBerkeberatan {
            text-align: justify;
            float: left;
            width: 108%;
        }

        .floatLeftText {
            text-align: justify;
            float: left;
            width: 108%;
        }

        #keterangan {
            text-align: justify-center;
            float: left;
            width: 108%;
        }

        .pdf-styled {
            width: 100%;
            height: 600px;
        }
    </style>
</head>

<body class="classbody">
    <div class="divbody">
        

     </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
